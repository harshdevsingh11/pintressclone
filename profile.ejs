
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="stylesheets/style3.css">  
</head>

<body>
  <header>
    <div class="navbar">
      <div class="logo">
        <img src="data:image/png;base64," alt="Logo"> <!-- Replace with your logo -->
      </div>
      <nav>
        <ul>
          <li><a href="/feed">Home</a></li>
          <li><a href="#">About</a></li>
          <li><a href="#">Services</a></li>
          <li><a href="/contact">Contact</a></li>
        </ul>
      </nav>
    <!-- Floating action button -->
    </div>
  </header>
    <div class="user-card">
      <!-- Card Cover/Avatar -->
      <div class="card-cover" style="background-image: url('https://cdn.tailkit.com/media/placeholders/photo-JgOeRuGD_Y4-800x400.jpg');">
        <div class="avatar-wrapper">
          <div class="avatar">
            <img src="data:image/jpeg;base64," alt="User Avatar" class="avatar-img">
          </div>
        </div>
      </div>
      <!-- END Card Cover/Avatar -->
  
      <!-- Card Body -->
      <div class="card-body">
        <h3 class="card-name"><%=user.username %></h3>
        <h3 class="fullname">@<%=user.username %></h3>
        <p class="card-info">Web Developer ∙ 12 Projects</p>
        <a href="/logout" class="button">logout</a>
        <p class="des">Upload any image</p>
        <form action="/upload" method="post" enctype="multipart/form-data">
          <input type="text" name="filecaption" placeholder="some caption">
          <input type="file" name="file">
          <input type="submit">
        </form>
      </div>
      <!-- END Card Body -->
    </div>
    
      <div class="container">
        <div class="cards flex">
          <% user.posts.forEach(function(post){
            %>
            <div class="card" style="width: 18rem;">
              <img src="/images/uploads/<%= post.image %>" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title"><%= post.postText%></h5>
                <a href="#" class="btn btn-primary">click to open</a>
              </div>
            </div>
        <%  }) %>
        </div>
      
      </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>